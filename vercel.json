{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://file-transfer-app-backend.vercel.app/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://file-transfer-app-backend.vercel.app/socket.io/:path*"
    }
  ]
}
